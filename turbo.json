{
    "$schema": "https://turborepo.com/schema.json",
    "ui": "tui",
    "tasks": {
        "test": {
            "dependsOn": []
        },
        "coverage": {
            "outputs": ["coverage/**"],
            "dependsOn": []
        },
        "lint": {
            "dependsOn": ["^build", "^lint"]
        },
        "build": {
            "outputs": ["dist/**"],
            "dependsOn": ["^build"]
        },
        "build:types": {
            "dependsOn": ["^build:types"]
        },
        "dev": {
            "persistent": true,
            "cache": false
        }
    }
}
